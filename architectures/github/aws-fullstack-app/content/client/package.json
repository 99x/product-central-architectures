{
    "name": "${{values.name}}",
    "version": "0.0.1",
    "private": true,
    "engines": {
        "node": ">=18.0.0",
        "yarn": ">=3.0.0"
    },
    "packageManager": "yarn@3.8.1",
    "scripts": {
        "dev": "nx run-many --target=serve --all --skip-nx-cache",
        "dev:${{values.coreAppName}}": "nx run ${{values.coreAppName}}:serve",
        "dev:${{values.remoteAppName}}": "nx run ${{values.remoteAppName}}:serve",
        "build:${{values.coreAppName}}": "nx run ${{values.coreAppName}}:build",
        "build:${{values.remoteAppName}}": "nx run ${{values.remoteAppName}}:build",
        "build:all": "nx run-many --target=build --all --skip-nx-cache",
        "graph:overview": "nx graph",
        "graph:out": "nx graph --file=output.json",
        "graph:${{values.coreAppName}}": "nx build ${{values.coreAppName}} --graph",
        "graph:${{values.remoteAppName}}": "nx build ${{values.remoteAppName}} --graph",
        "graph:build:all": "nx run-many --target=build --graph",
        "graph:build:affected": "nx affected --target=build --graph",
        "lint:${{values.coreAppName}}": "nx run ${{values.coreAppName}}:lint",
        "lint:${{values.remoteAppName}}": "nx run ${{values.remoteAppName}}:lint",
        "lint:all": "nx run-many --target=lint --all --skip-nx-cache",
        "test:${{values.coreAppName}}": "nx run ${{values.coreAppName}}:test",
        "test:${{values.remoteAppName}}": "nx run ${{values.remoteAppName}}:test",
        "test:environments": "nx run environments:test",
        "test:all": "nx run-many --target=test --all --skip-nx-cache",
        "e2e:${{values.coreAppName}}": "nx run ${{values.coreAppName}}:e2e",
        "e2e:${{values.remoteAppName}}": "nx run ${{values.remoteAppName}}:e2e",
        "e2e:all": "nx run-many --target=e2e --all --skip-nx-cache",
        "affected:${{values.coreAppName}}": "nx affected --target=${{values.coreAppName}}",
        "affected:${{values.remoteAppName}}": "nx affected --target=${{values.remoteAppName}}",
        "affected:build": "nx affected:build",
        "affected:e2e": "nx affected:e2e",
        "affected:test": "nx affected:test",
        "affected:lint": "nx affected:lint ",
        "generate": "./tools/scripts/generate.sh",
        "clear:nx:cache": "nx reset",
        "repair:nx": "nx repair",
        "postinstall": "husky || true",
        "prepare": "husky install",
        "prettier:check": "prettier --check .",
        "prettier:fix": "prettier --write ."
    },
    "dependencies": {
        "@module-federation/nextjs-mf": "^8.3.13",
        "aws-sdk": "^2.1633.0",
        "next": "14.0.4",
        "nextjs-toploader": "^1.6.12",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "tslib": "^2.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.14.5",
        "@babel/preset-react": "^7.14.5",
        "@nrwl/linter": "19.0.2",
        "@nrwl/node": "19.0.2",
        "@nx/devkit": "19.0.2",
        "@nx/eslint": "19.0.2",
        "@nx/eslint-plugin": "19.0.2",
        "@nx/jest": "19.0.2",
        "@nx/js": "19.0.2",
        "@nx/next": "19.0.2",
        "@nx/node": "19.0.2",
        "@nx/playwright": "19.0.2",
        "@nx/react": "19.0.2",
        "@nx/web": "19.0.2",
        "@nx/webpack": "19.0.2",
        "@nx/workspace": "19.0.2",
        "@playwright/test": "^1.36.0",
        "@swc-node/register": "~1.8.0",
        "@swc/cli": "~0.3.12",
        "@swc/core": "~1.3.85",
        "@swc/helpers": "~0.5.2",
        "@testing-library/react": "^15.0.7",
        "@types/jest": "^29.4.0",
        "@types/node": "18.16.9",
        "@types/react": "18.3.1",
        "@types/react-dom": "18.3.0",
        "@typescript-eslint/eslint-plugin": "^7.3.0",
        "@typescript-eslint/parser": "^7.3.0",
        "autoprefixer": "10.4.13",
        "babel-jest": "^29.4.1",
        "concurrently": "^8.0.0",
        "eslint": "~8.57.0",
        "eslint-config-next": "14.0.4",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-playwright": "^0.15.3",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "husky": "^8.0.0",
        "jest": "^29.4.1",
        "jest-environment-jsdom": "^29.4.1",
        "jest-environment-node": "^29.4.1",
        "lint-staged": "^15.0.0",
        "nx": "19.0.2",
        "postcss": "8.4.21",
        "prettier": "^2.6.2",
        "tailwindcss": "3.2.7",
        "ts-jest": "^29.1.0",
        "ts-node": "10.9.1",
        "typescript": "~5.4.2",
        "webpack-cli": "^5.1.4"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,mjs,cjs}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,md}": [
            "prettier --write"
        ]
    }
}
